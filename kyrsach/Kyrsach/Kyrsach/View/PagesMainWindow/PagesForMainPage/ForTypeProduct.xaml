        <Button Grid.Row="0" Grid.Column="2" Style="{StaticResource MainButtonsPage}" Content="Спальная орта"/>
        <Button Grid.Row="1" Grid.Column="2" Style="{StaticResource MainButtonsPage}" Content="Мягкая мебель тревел М"/>
