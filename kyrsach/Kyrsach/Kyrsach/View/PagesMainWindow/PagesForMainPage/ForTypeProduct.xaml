        <Button Grid.Row="3" Grid.Column="2" Style="{StaticResource MainButtonsPage}" Content="Мягкая мебель Портленд"/>
        <Button Grid.Row="4" Grid.Column="2" Style="{StaticResource MainButtonsPage}" Content="Мягкая мебель Оникс"/>
        <Button Grid.Row="5" Grid.Column="2" Style="{StaticResource MainButtonsPage}" Content="Спальня Лорена"/>
        <Button Grid.Row="6" Grid.Column="2" Style="{StaticResource MainButtonsPage}" Content="Спальня Лолита"/>
        <Button Grid.Row="7" Grid.Column="2" Style="{StaticResource MainButtonsPage}" Content="Мягкая мебель Пума"/>
